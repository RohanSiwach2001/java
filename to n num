import java.util.Scanner;
public class print{
public static void main(String args[]){
    Scanner sc = new Scanner (System.in);
    System.out.println ("value of n");
    int n= sc.nextInt();
    System.out.println ("numbers are");
    for (int i=1; i<n ; i++)
    {
        System.out.println (i);
        
    }
} 
}
